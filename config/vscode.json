  "editor.fontFamily": "'Droid Sans Mono', 'Courier New', monospace, 'Droid Sans Fallback'",
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "editor.scrollBeyondLastLine": false,
